@manual{diodes_ap62201,
  title        = {AP62201: 4.2V to 18V Input, 2A Low IQ Synchronous Buck Converter},
  author       = {Diodes Incorporated},
  year         = {2021},
  url          = {https://www.diodes.com/part/view/AP62201/},
  note         = {Accessed: 2025-04-17},
  description  = {The AP62201 is a 2A synchronous buck converter with a wide input voltage range of 4.2V to 18V. It features a 0.8V ±1% reference voltage, 135μA low quiescent current, and 750kHz switching frequency. The device integrates a 90mΩ high-side power MOSFET and a 65mΩ low-side power MOSFET for high-efficiency step-down DC-DC conversion. It offers protection features such as undervoltage lockout (UVLO), cycle-by-cycle valley current limit, and thermal shutdown. The AP62201 is available in SOT563 and TSOT26 packages.}
}